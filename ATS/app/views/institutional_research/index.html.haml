%h1 list of students

%table#student.table
	%thead
		%tr
			%th= 'First Name'
			%th= 'Last Name'
			%th= 'Middle Name'
			%th= 'Email'
			%th More Info

	%tbody
		- @students.each do |student|
			%tr
				%td= student.first_name
				%td= student.last_name
				%td= student.middle_name
				%td= student.primary_email
	
%br
%h3 Import Students


= form_tag import_institutional_research_index_path, multipart: true do	
	= file_field_tag :file
	= submit_tag "Upload Students"