%h2 Create New Department

= form_tag departments_path, :method => :post do

  = render :partial => 'departments_form_fields'
  = render :partial => 'colleges/colleges_dropdown'

  %div{:class => 'form-group'}
    = submit_tag 'Save Changes', :class => 'btn btn-primary'
  
%p
  = render partial: 'shared/cancel_button', locals: {path: departments_path}  
